{
  "name": "@habbora/bun-websocket-proxy",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "bun": ">=1.3.0"
  },
  "scripts": {
    "build": "bun run build:lib && bun run build:types",
    "build:lib": "bun build src/index.ts --outdir dist --target bun",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "prepare": "bun run build",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "typescript": "^5.9.3"
  }
}